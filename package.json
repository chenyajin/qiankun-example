{
  "name": "qiankun-example",
  "version": "0.0.1",
  "description": "qiankun demo",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:qiankun-base-vue3": "cd qiankun-base-vue3 && pnpm i",
    "install:qiankun-micro-vue3": "cd qiankun-micro-vue3 && pnpm i",
    "install:qiankun-micro-vue2": "cd qiankun-micro-vue2 && pnpm i",
    "start": "npm-run-all --parallel start:*",
    "start:qiankun-base-vue3": "cd qiankun-base-vue3 && npm run dev",
    "start:qiankun-micro-vue3": "cd qiankun-micro-vue3 && npm run dev",
    "start:qiankun-micro-vue2": "cd qiankun-micro-vue2 && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chenyajin/qiankun-example.git"
  },
  "keywords": [
    "qiankun",
    "qiankun-example",
    "demo"
  ],
